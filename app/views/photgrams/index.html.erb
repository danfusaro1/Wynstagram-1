<p id="notice"><%= notice %></p>

<h1>Listing Photgrams</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Picture</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @photgrams.each do |photgram| %>
      <tr>
        <td><%= photgram.description %></td>
        <td><%= photgram.picture %></td>
        <td><%= link_to 'Show', photgram %></td>
        <td><%= link_to 'Edit', edit_photgram_path(photgram) %></td>
        <td><%= link_to 'Destroy', photgram, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Photgram', new_photgram_path %>
